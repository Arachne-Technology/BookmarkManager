{
  "name": "bookmark-parser-backend",
  "version": "1.0.0",
  "description": "Backend API for BookmarkParser web application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "biome check src",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write src",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "multer": "^2.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "@vitest/ui": "^3.2.4",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}
